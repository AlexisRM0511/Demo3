    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.solicitud ){",
        "\treturn {",
        "\t\t//map solicitud variable to expected task contract input",
        "\t\tsolicitudBInput: {",
        "\t\t\tdescription: $data.solicitud.description !== undefined ? $data.solicitud.description : null,",
        "\t\t\tstartDate: $data.solicitud.startDate !== undefined ? $data.solicitud.startDate : null,",
        "\t\t\tendDate: $data.solicitud.endDate !== undefined ? $data.solicitud.endDate : null,",
        "\t\t\tisComplete: $data.solicitud.isComplete !== undefined ? $data.solicitud.isComplete : null,",
        "\t\t\tisApproved: $data.solicitud.isApproved !== undefined ? $data.solicitud.isApproved : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
